[ Rozklad liczby na czynniki pierwsze ]
DECLARE
    n; m; reszta; potega; dzielnik;
IN
    n  ASSIGN  236;
    dzielnik  ASSIGN  2;
    m  ASSIGN  dzielnik  TIMES   dzielnik;
    WHILE n >= m DO
        potega  ASSIGN  0;
        reszta  ASSIGN  n  MOD  dzielnik;
        WHILE reszta = 0 DO
            n  ASSIGN  n DIV dzielnik;
            potega  ASSIGN  potega PLUS 1;
            reszta  ASSIGN  n  MOD  dzielnik;
        ENDWHILE
        IF potega > 0 THEN [ czy znaleziono dzielnik ]
            WRITE dzielnik;
            WRITE potega;
        ELSE
            dzielnik  ASSIGN  dzielnik PLUS 1;
            m  ASSIGN  dzielnik  TIMES   dzielnik;
        ENDIF
    ENDWHILE
    IF n != 1 THEN [ ostatni dzielnik ]
        WRITE n;
        WRITE 1;
    ENDIF
END
